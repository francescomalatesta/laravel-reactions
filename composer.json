{
    "name": "francescomalatesta/laravel-reactions",
    "type": "library",
    "description": "",
    "keywords": [
        "laravel",
        "package",
	"reactions",
	"like",
	"love",
	"eloquent",
	"trait"
    ],
    "homepage": "https://github.com/francescomalatesta/laravel-reactions",
    "license": "MIT",
    "authors": [
        {
            "name": "Francesco Malatesta",
            "email": "hellofrancesco@gmail.com",
            "homepage": "http://hellofrancesco.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "illuminate/database": "~5.1",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "orchestra/testbench": "3.1.*",
        "mockery/mockery": "^0.9.4",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "FrancescoMalatesta\\LaravelReactions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FrancescoMalatesta\\LaravelReactions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
